
public class PlayerScores 
{
	private int playerID;
	private int quizId;
	private int score; 
	
	public PlayerScores(int quizId, int score, int playerID)
	{
		this.playerID = playerID;
		this.quizId = quizId;
		this.score = score;
	}
	
	public int getQuizID()
	{
		return quizId;
	}
	
	public int getScore()
	{
		return score;
	}
	
	
	public int getPlayerId()
	{
		return playerID;
	}
	
}
